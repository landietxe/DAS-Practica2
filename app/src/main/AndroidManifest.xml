<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.practica2">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:label="Bookmarked"
        android:theme="@style/TemaDAS">
        <activity android:name="com.example.practica2.Actividades.PreferenciasActivity"/>
        <activity android:name="com.example.practica2.Actividades.RegisterActivity" />
        <activity android:name="com.example.practica2.Actividades.InfoLibroBiblioteca" />
        <activity android:name="com.example.practica2.Actividades.MainActivity" />
        <activity android:name="com.example.practica2.Actividades.InfoLibro" />
        <activity android:name="com.example.practica2.Actividades.MainActivityBiblioteca" />
        <activity android:name="com.example.practica2.Actividades.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>